<root>
	<windowclass name="charsheet" merge="join">
		<sheetdata>
			<string_labeled name="name">
				<anchored to="overviewframe" position="insidetopleft" offset="15,10" height="20">
					<right offset="-15" />
				</anchored>
				<labelres>char_label_name</labelres>
			</string_labeled>

			<resize_charsheet />
			<close_charsheet />
		</sheetdata>
	</windowclass>
</root>
